{
  "author": "CandleSmartHome.com",
  "content_scripts": [
    {
      "css": [
        "css/extension.css"
      ],
      "js": [
        "js/extension.js",
        "js/shaka.js"
      ]
    }
  ],
  "description": "Turn a Pi into a security camera or smart doorbell",
  "gateway_specific_settings": {
    "webthings": {
      "exec": "python3 {path}/main.py",
      "primary_type": "extension",
      "strict_max_version": "*",
      "strict_min_version": "0.10.0"
    }
  },
  "homepage_url": "https://github.com/createcandle/candlecam",
  "id": "candlecam",
  "license": "None",
  "manifest_version": 1,
  "name": "Candle cam",
  "options": {
    "default": {
      "Debug": false
    },
    "schema": {
      "properties": {
        "Debug": {
          "description": "Enable this to see much more detail in the internal log about what this add-on is doing.",
          "type": "boolean"
        }
      },
      "type": "object"
    }
  },
  "short_name": "candlecam",
  "version": "0.0.5",
  "web_accessible_resources": [
    "css/*.css",
    "images/*.svg",
    "photos/*.gif",
    "photos/*.jpg",
    "photos/*.jpeg",
    "stream/*.mpd",
    "stream/*.m4s",
    "stream/*.m3u8",
    "stream/*.mjpg",
    "stream/*.mjpeg",
    "js/*.js",
    "views/*.html"
  ]
}